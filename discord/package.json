{
  "name": "kimaki",
  "module": "index.ts",
  "type": "module",
  "version": "0.4.55",
  "scripts": {
    "dev": "tsx --env-file .env src/cli.ts",
    "prepublishOnly": "pnpm generate && pnpm tsc",
    "dev:bun": "DEBUG=1 bun --env-file .env src/cli.ts",
    "watch": "tsx scripts/watch-session.ts",
    "generate": "prisma generate && pnpm generate:sql",
    "generate:sql": "rm -f dev.db && prisma db push --url 'file:dev.db' --accept-data-loss && echo '-- This file is generated by pnpm generate:sql. Do not edit manually.' > src/schema.sql && sqlite3 dev.db '.schema' >> src/schema.sql",
    "pcm-to-mp3": "bun scripts/pcm-to-mp3",
    "test": "vitest",
    "test:send": "tsx send-test-message.ts",
    "register-commands": "tsx scripts/register-commands.ts",
    "format": "oxfmt src"
  },
  "repository": "https://github.com/remorses/kimaki",
  "bin": "bin.js",
  "files": [
    "dist",
    "src",
    "bin.js"
  ],
  "devDependencies": {
    "@opencode-ai/plugin": "^1.1.12",
    "@types/better-sqlite3": "^7.6.13",
    "@types/bun": "latest",
    "@types/heic-convert": "^2.1.0",
    "@types/js-yaml": "^4.0.9",
    "@types/ms": "^2.1.0",
    "@types/node": "^24.3.0",
    "prisma": "^7.3.0",
    "tsx": "^4.20.5"
  },
  "dependencies": {
    "@clack/prompts": "^1.0.0",
    "@discordjs/voice": "^0.19.0",
    "@google/genai": "^1.34.0",
    "@libsql/client": "^0.15.15",
    "@opencode-ai/sdk": "^1.1.31",
    "@prisma/adapter-libsql": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@purinton/resampler": "^1.0.4",
    "@xmorse/cac": "^6.0.7",
    "ai": "^5.0.114",
    "better-sqlite3": "^12.3.0",
    "diff": "^8.0.3",
    "discord.js": "^14.16.3",
    "domhandler": "^5.0.3",
    "errore": "workspace:^",
    "glob": "^13.0.0",
    "htmlparser2": "^10.0.0",
    "js-yaml": "^4.1.0",
    "marked": "^16.3.0",
    "mime": "^4.1.0",
    "picocolors": "^1.1.1",
    "pretty-ms": "^9.3.0",
    "ripgrep-js": "^3.0.0",
    "string-dedent": "^3.0.2",
    "traforo": "workspace:*",
    "undici": "^7.16.0",
    "xdg-basedir": "^5.1.0",
    "zod": "^4.2.1"
  },
  "optionalDependencies": {
    "@discordjs/opus": "^0.10.0",
    "heic-convert": "^2.1.0",
    "prism-media": "^1.3.5",
    "sharp": "^0.34.5"
  }
}
